<h1>Sign in</h1>

<p>
Currently signed in as:
<%= session[:current_user_name] %>
</p>
<%= form_with(model: @user, url:'/sign_in', method: 'get') do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
    <%= form.label :id %>
    <%= form.text_field :id %>

  </div>

  <div class="actions">
    <%= form.submit "Sign in" %>
  </div>
<% end %>

<br />
<br />
<br />
<div >
    New Event <br />
    <%= link_to 'new', events_path %>
</div>